@{ ViewBag.Title = "OrderComplaint";}
<script src="~/Scripts/jquery.hiMallDatagrid.js"></script>
<script src="~/Scripts/jquery.himallLinkage.nocreate.js"></script>
<script src="~/Scripts/jquery.placeholder.min.js"></script>
<script src="~/Scripts/bootstrap.autocomplete.js"></script>
<script src="~/Areas/Admin/Scripts/accountManagement.js"></script>

<div class="container">
    <ul class="nav nav-tabs-custom clearfix">
        <strong>结算管理</strong>
        <li value="0" class="active" type="statusTab"><a onclick="Query(0)">待结算</a></li>
        <li value="1" type="statusTab"><a onclick="Query(1)">已结算</a></li>
        <li><a href="@Url.Action("SetSettlementWeek")">结算周期设置</a></li>
        <em class="primary-btn">
            <span class="glyphicon glyphicon-question-sign"></span>
            <div class="primary">
                <h3>温馨  ：</h3>
                <span>*</span><p>管理平台店铺每月结算账单</p>
            </div>
        </em>
    </ul>
    <div class="search-box only-line">

        <form class="custom-inline" role="form">

            <div class="form-group">
                <label class="label-inline" for="">店铺名称</label>
                <input class="form-control input-ssm" type="text" id="txtShopName">
            </div>
            <button type="button" class="btn btn-primary btn-sm" id="searchButton" >搜 索</button>
            <a class="btn-ssm" href="#" onclick="exportExcel()" id="exceptExcelA">导出EXCEL</a>
            <span style="color:#e3393c; margin-left:50px; font-size:14px;">本期应结=商品实付总额 + 运费 - 佣金 - 退款金额 + 退还佣金 - 服务费用</span>
        </form>
    </div>
    <table id="list" class="table"></table>
</div>
