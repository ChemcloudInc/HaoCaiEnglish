@{
   Layout = null;
}
<!doctype html>
<html lang="zh-CN">
<head>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<title>Merchant Settled Agreement</title> @*商家入住协议*@
<link rel="stylesheet" type="text/css" href="~/Content/bootstrap.min.css" />
<link rel="stylesheet" type="text/css" href="~/Content/inner-custom.css" />
<script type="text/javascript" src="~/Scripts/jquery-1.11.1.min.js"></script>
<script type="text/javascript" src="~/Scripts/bootstrap.min.js"></script>
<!--[if lt IE 9]>
  <script src="http://cdn.bootcss.com/respond.js/1.4.2/respond.min.js"></script>
<![endif]-->
<script type="text/javascript" src="~/Scripts/jquery.nicescroll.min.js"></script>
</head>
<body>

<div class="container">
    
    <form class="form-horizontal" role="form" method="post" action="JoinSeller_Step1">
    	<h3 class="art-title">Settled Agreement</h3> @*入驻协议*@
        <div class="form-group">
        	<div class="col-sm-10 col-sm-offset-1">

        		<p>Please read these terms and conditions carefully. Please pay attention to provisons that excludes or limit liability and
terms of governing low and  jurisdiction.</p>  @*协议内容*@
            </div>
        </div>
        <div class="form-group">
            <div class="col-sm-offset-1 col-sm-4">
                <label><input type="checkbox" id="agree" name="agree" /> I have read the agreeement and agree to them.</label>  @*我已阅读并同意以上协议*@
            </div>
        </div>
        
        <div class="form-group">
            <div class="col-sm-offset-1 col-sm-10">
            	<button type="submit" class="btn btn-primary">Next Step</button> @*下 一 步*@
            </div>
        </div>
    </form>
</div>

</body>
</html>